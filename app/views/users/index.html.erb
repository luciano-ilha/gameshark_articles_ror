<h1>This is the Homepage</h1>

<% if logged_in? %>
  <h4><%= current_user.username %></h4>
  <%= link_to "User", welcome_path %>
  <%= button_to "Logout", '/logout', method: :delete %><br>
<% else %>
  <%= button_to "Sign Up", '/users/new', method: :get %><br>
  <%= button_to "Login", '/login', method: :get %><br>
<% end %>
